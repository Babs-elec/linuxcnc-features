  <feature src="probe-hole.ini" expanded="False" help="&lt;big&gt;Probe hole&lt;/big&gt;&#10;.Probes hole to find it&amp;#39;s center" icon="icons/probe-hole.png" image="images/probe-hole.png" after="" order="['param_cx', 'param_cy', 'param_r', 'param_func', 'param_feed', 'param_latch', 'param_latch_feed', 'param_depth', 'param_rappid']" call="&#10;(Probe hole)&#10;&lt;eval&gt;self.include_once(&quot;probe.ngc&quot;)&lt;/eval&gt;&#10;G0 Z[#param_rappid]&#10;G0 X[#param_cx] Y[#param_cy]&#10;G0 Z[#param_depth]&#10;O&lt;probe&gt; call [#param_func] [#param_feed] [#param_latch] [#param_latch_feed] [#param_r] [0] [0]&#10;(5061-5069 Coordinates of a &quot;G38.2&quot; Probe result - X, Y, Z, A, B, C, U, V &amp; W. Volatile.)&#10;#&lt;x&gt; = #5061&#10;G0 X[#param_cx] Y[#param_cy]&#10;O&lt;probe&gt; call [#param_func] [#param_feed] [#param_latch] [#param_latch_feed] [-#param_r] [0] [0]&#10;#&lt;_probe_size_x&gt; = [#&lt;x&gt;-#5061]&#10;#&lt;x&gt; = [[#&lt;x&gt;+#5061]/2]&#10;G0 X[#param_cx] Y[#param_cy]&#10;G0 X[#&lt;x&gt;]&#10;O&lt;probe&gt; call [#param_func] [#param_feed] [#param_latch] [#param_latch_feed] [0] [#param_r] [0]&#10;#&lt;y&gt; = #5062&#10;&#10;G0 X[#&lt;x&gt;] Y[#param_cy]&#10;O&lt;probe&gt; call [#param_func] [#param_feed] [#param_latch] [#param_latch_feed] [0] [-#param_r] [0]&#10;#&lt;_probe_size_y&gt; = [#&lt;y&gt;-#5062]&#10;#&lt;y&gt; = [[#&lt;y&gt;+#5062]/2]&#10;G0 X#&lt;x&gt; Y#&lt;y&gt;&#10;G0 Z[#param_rappid]&#10;&#10;(Probe hole end)&#10;" selected="False" path="2" definitions="" before="&#10;" type="Probe-hole" id="Probe-hole-0001" name="Probe-hole 0001">
    <param tool_tip="Approximate CX" selected="False" name="CX" expanded="False" value="0" call="#param_cx" path="2:0" type="float"/>
