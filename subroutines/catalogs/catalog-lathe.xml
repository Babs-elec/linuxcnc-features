<xml>
	<defaults src="lathe/lathe-defaults.ngc"/>
	
	<group name="Draw" icon="images/drawing.png">
		<sub name="Goto" icon="images/goto.png" sub="draw/goto.ini"/>
		<sub name="Line to" icon="images/line-to.png" sub="draw/line-to.ini"/>
		<sub name="Arc to (X,Y),R" icon="images/arc-to-xyr.png" sub="draw/arc-to-xyr.ini"/>
		<sub name="Arc to (X,Y),(I,J)" icon="images/arc-to.png" sub="draw/arc-to.ini"/>
	</group>
	 
	<group name="Lathe OD/ID" icon="images/od.png">
		<sub name="OD-simple" sub="lathe/od-simple.ini"/>	
		<sub name="OD" sub="lathe/od.ini"/>	
	</group>
		
	<group name="Grouping" icon="images/array.png"> 
		<sub name="Lathe multipass" sub="lathe/lathe-multipass.ini"/>
		<sub name="Group" icon="images/group.png" sub="group.ini"/>
		<sub name="Array" icon="images/array.png" sub="array.ini"/>
		<sub name="Circular array" icon="images/circular-array.png" sub="circular-array.ini"/>
	</group>
	
	<group name="Tool" icon="images/lathe-tool.png">
		<sub name="Set lathe cuting params" sub="lathe/lathe-cutting-params.ini"/>
	</group>

	<group name="Gcode" icon="images/gcode.png">
		<sub name="Custom Gcode" icon="images/gcode.png" sub="gcode/gcode.ini"/>
		<sub name="G91 Group" icon="images/g91.png" sub="gcode/g91-group.ini"/>
		
	</group>

	<group name="Probing" icon="images/probe-cube.png">
		<sub name="Probe down" sub="probe-down.ini" icon="images/probe-down.png"/>
		<sub name="Probe hole" sub="probe-hole.ini" icon="images/probe-hole.png"/>
		<sub name="Probe cube" sub="probe-cube.ini" icon="images/probe-cube.png"/>
		<sub name="Set probing params" icon="images/probe.png" sub="set-probing-params.ini"/>
	</group>
</xml>

<!-- Do not forget to close <sub /> tags! -->	
