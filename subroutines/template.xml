    <param tool_tip="Approximate Radius (must be larger than hole)" selected="False" name="R+" expanded="False" value="100" call="#param_r" path="2:2" type="float"/>
    <param tool_tip="&#10;.G38.2 - probe toward workpiece, stop on contact, signal error if failure&#10;.G38.3 - probe toward workpiece, stop on contact&#10;.G38.4 - probe away from workpiece, stop on loss of contact, signal error if failure&#10;.G38.5 - probe away from workpiece, stop on loss of contact&#10;.(Do not write G, just '38.2'.)" selected="False" name="Gcode" expanded="False" value="38.2" call="#param_func" path="2:3" type="float" icon="icons/gcode.png"/>
